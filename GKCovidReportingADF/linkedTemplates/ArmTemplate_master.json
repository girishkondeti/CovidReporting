{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "GKCovidReportingADF"
		},
		"ls_ablob_covidreportingsa_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_ablob_covidreportingsa'"
		},
		"ls_adls_covidreporting_dl_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_covidreporting_dl'"
		},
		"ls_adls_covidreporting_dl_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://gkcovidreportingdl.dfs.core.windows.net"
		},
		"ls_http_opendata_ecdc_europa_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com"
		},
		"Tr_copyfile_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/6d53f9f4-c0aa-476f-a768-52917162c8b5/resourceGroups/GKCoidReportingRG/providers/Microsoft.Storage/storageAccounts/gkcovidreportingsa"
		},
		"tr_ecdc_data_copy_properties_pl_ingest_ecdc_files_parameters_SinkFileName": {
			"type": "string",
			"defaultValue": "hospital_admissions.csv"
		},
		"tr_ecdc_data_copy_properties_pl_ingest_ecdc_files_parameters_RelativePath": {
			"type": "string",
			"defaultValue": "cloudboxacademy/covid19/main/ecdc_data/hospital_admissions.csv"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "GKCovidReportingADF_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_ablob_covidreportingsa_connectionString": {
						"value": "[parameters('ls_ablob_covidreportingsa_connectionString')]"
					},
					"ls_adls_covidreporting_dl_accountKey": {
						"value": "[parameters('ls_adls_covidreporting_dl_accountKey')]"
					},
					"ls_adls_covidreporting_dl_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_covidreporting_dl_properties_typeProperties_url')]"
					},
					"ls_http_opendata_ecdc_europa_properties_typeProperties_url": {
						"value": "[parameters('ls_http_opendata_ecdc_europa_properties_typeProperties_url')]"
					},
					"Tr_copyfile_properties_typeProperties_scope": {
						"value": "[parameters('Tr_copyfile_properties_typeProperties_scope')]"
					},
					"tr_ecdc_data_copy_properties_pl_ingest_ecdc_files_parameters_SinkFileName": {
						"value": "[parameters('tr_ecdc_data_copy_properties_pl_ingest_ecdc_files_parameters_SinkFileName')]"
					},
					"tr_ecdc_data_copy_properties_pl_ingest_ecdc_files_parameters_RelativePath": {
						"value": "[parameters('tr_ecdc_data_copy_properties_pl_ingest_ecdc_files_parameters_RelativePath')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}